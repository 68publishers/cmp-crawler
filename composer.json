{
    "name": "68publishers/cmp-crawler",
    "description": "Consent Management Platform - Cookie Crawler",
    "keywords": ["68publishers", "cmp", "cookie", "crawler"],
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.1",
        "ext-dom": "*",
        "ext-json": "*",
        "68publishers/doctrine-bridge": "^1.0",
        "68publishers/environment": "^1.0",
        "68publishers/file-storage": "^1.0",
        "68publishers/health-check": "^1.0",
        "68publishers/omni": "0.2.x-dev",
        "contributte/apitte": "^0.10.2",
        "contributte/console": "^0.9.2",
        "fmasa/messenger": "dev-feature/handler-options as 2.0.0",
        "nette/bootstrap": "^3.2",
        "nette/di": "^3.1",
        "nettrine/migrations": "^0.8.0",
        "nettrine/orm": "^0.8.3",
        "ramsey/uuid": "^4.7",
        "spatie/crawler": "^7.1",
        "symfony/validator": "^6.2",
        "tracy/tracy": "^2.10"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "3.14.2",
        "kubawerlos/php-cs-fixer-custom-fixers": "^3.11",
        "phpstan/phpstan": "^1.9",
        "phpstan/phpstan-nette": "^1.2",
        "roave/security-advisories": "dev-latest",
        "symplify/phpstan-rules": "^11.2"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/tg666/messenger"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "68publishers/environment": true
        }
    }
}
